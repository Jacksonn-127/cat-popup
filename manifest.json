{
  "manifest_version": 3,
  "name": "Silly Cat Viewer",
  "version": "1.0",
  "description": "Displays a silly cat picture.",
  "permissions": [
    "activeTab" 
  ],
  "web_accessible_resources": [
    {
      "resources": ["images/*"], 
      "matches": ["<all_urls>"] 
    }
  ], 
  "action": {
    "default_popup": "cat.html",
    "default_icon": "default_icon.png"
  }
}